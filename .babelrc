{
    "presets": [
        "react",
        "es2015"
    ],
    "plugins": [
        "transform-class-properties",
        "react-hot-loader/babel"
    ],
    "env": {
        "test": {
            "plugins": [
                "istanbul"
            ]
        },
        "production": {
            "presets": [
                [
                    "es2015",
                    {
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                "transform-class-properties"
            ]
        }
    }
}
